{
  "name": "botp-core",
  "version": "0.0.1",
  "description": "solidity smart contracts for block based otp on confidential chains",
  "main": "index.js",
  "scripts": {
    "sapphire": "sudo ./sapphire.sh",
    "test": "hardhat test ./src/test/base.spec.ts --network sapphire_local",
    "test:hardhat": "hardhat test ./src/test/base.spec.ts --network hardhat",
    "test:experiment": "hardhat test ./src/test/experiment.spec.ts",
    "lint:sol": "solhint  contracts/**/*.sol --fix",
    "lint:ts": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore --ext .js,.ts . --fix",
    "prettier": "prettier --config ./.prettierrc.json --ignore-path ./.prettierignore --write \"**/*.{js,json,md,sol,ts}\"",
    "clean": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat clean",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "local:deploy": "hardhat run --network localhost src/scripts/deploy_local.ts",
    "hardhat:node": "hardhat node",
    "client": "yarn --cwd ../botp-client dev",
    "local:deploy:client": "yarn local:deploy && yarn client",
    "analyze": "code4rena-report-generator"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0xkrauser/botp-core.git"
  },
  "author": {
    "name": "Johannes Krauser III",
    "email": "krauser@hey.com",
    "url": "https://github.com/0xKrauser"
  },
  "keywords": [
    "ethereum",
    "smart-contracts",
    "solidity"
  ],
  "bugs": {
    "url": "https://github.com/0xkrauser/botp-core/issues"
  },
  "homepage": "https://github.com/0xkrauser/botp-core#readme",
  "private": true,
  "devDependencies": {
    "@codechecks/client": "^0.1.12",
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/providers": "^5.5.0",
    "@graphprotocol/hardhat-graph": "^0.1.0-alpha.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@oasisprotocol/sapphire-contracts": "^0.2.1",
    "@oasisprotocol/sapphire-hardhat": "^1.0.3",
    "@typechain/ethers-v5": "^8.0.1",
    "@typechain/hardhat": "^3.0.0",
    "@types/chai": "^4.2.22",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vercel/node": "^1.12.1",
    "chai": "^4.3.4",
    "chai-ethers": "^0.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ethereumjs-util": "^7.1.3",
    "ethers": "^5.6.4",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.11.1",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-deploy": "^0.9.4",
    "hardhat-deploy-ethers": "^0.3.0-beta.11",
    "hardhat-gas-reporter": "^1.0.4",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "shelljs": "^0.8.4",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.21",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.3.0",
    "typechain": "^6.0.2",
    "typescript": "^4.5.4",
    "vercel": "^23.1.2"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.4.1",
    "@openzeppelin/contracts": "^4.3.2"
  }
}
